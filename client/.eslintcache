<Feeds />\n\n            </section>\n\n            <div className=\"addPost-modal\">\n\n                <Modal\n                    isOpen={modalIsOpen}\n                    onRequestClose={closeModal}\n                    className=\"modal\"\n                    overlayClassName=\"modal-overlay\"\n                >\n                    <button className=\"closemodal-btn\">\n                    close\n                    \n                    </button>\n                   <div className=\"createPost-form\">\n                    <form \n                    action=\"/api/route/post\" \n                    method=\"post\" \n                    encType=\"multipart/form-data\"\n                    onSubmit={handleFormSubmit} >\n                    <div className=\"caption\">\n                    <input\n                     type=\"text\" \n                     placeholder=\"Caption\"\n                      name=\"caption\" \n                      value={caption}  \n                      onChange={(e)=> setCaption(e.target.value)}\n                      />\n                    </div>\n                    <div className=\"file-input\">\n                    <span>Uplaod Image</span>\n                    <input \n                    type='file' \n                    name=\"image\"\n                    placeholder=\"select image to upload\"\n                    onChange={handleFileChange}\n                    />\n    \n                    </div>\n                    <input type=\"submit\" value=\"Post\" />\n                    </form>\n\n                   \n                   </div>\n                </Modal>\n            </div>\n\n            <ToastContainer/>\n        </div>\n    )\n};\n\nexport default connect(null, {createPost, getposts})(withRouter(Main));\n","/Users/mac/Documents/mern stack/photoShare/client/src/redux/reducers/posts/postReducer.js",["83"],"import { PostTypes } from '../../actions/post/post.types';\nimport { userTypes } from '../../actions/user/user.types'\nconst initState = {\n    posts: [],\n    post: null,\nloading: true,\n    error: {}\n\n};\n\nconst postReducer = (state = initState, action) => {\n    const { type, payload } = action\n    switch (type) {\n        case PostTypes.GET_POSTS:\n            return {\n                ...state,\n                posts: payload,\n                loading: false\n\n            };\n        case PostTypes.GET_POST:\n            return {\n                ...state,\n                post: payload,\n                loading: false\n            };\n        case PostTypes.ADD_POST:\n            return {\n                ...state,\n                loading: false,\n                posts: [...state.posts, payload]\n\n            };\n        case PostTypes.DELETE_POST:\n            return {\n                ...state,\n                posts: state.posts.flter((post) => post.id !== payload)\n            };\n        case PostTypes.POST_ERROR:\n            return {\n                ...state,\n                loading: false,\n                error: payload\n\n            };\n        case PostTypes.UPDATE_LIKES:\n            return {\n                ...state,\n                loading: false,\n                posts: state.posts.map((post) => {\n                    return post.id === payload.id ? { ...post, likes: payload } : post\n                })\n\n\n            };\n        case PostTypes.ADD_COMMENT:\n            return {\n                ...state,\n                loading: false,\n                post: { ...state.post, comments: payload }\n\n\n            };\n        case PostTypes.REMOVE_COMMENT:\n            return {\n                ...state,\n                laoding: false,\n                post: {\n                    ...state.post,\n                    comments: state.post.comments.filter((comment) => comment.id !== payload)\n\n                }\n            }\n\n        default:\n            return state;\n    }\n\n};\nexport default postReducer;","/Users/mac/Documents/mern stac